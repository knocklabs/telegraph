{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"],
      "cache": false
    },
    "build:packages": {
      "dependsOn": ["^build:packages"],
      "inputs": ["src/**"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "build:config": {
      "dependsOn": ["^build:config"],
      "inputs": ["src/**"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "dev:packages": {
      "dependsOn": ["^build:config"],
      "cache": false,
      "persistent": true
    },
    "dev:examples": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "test": {
      "dependsOn": ["build"]
    },
    "test:watch": {
      "cache": false
    },
    "coverage": {
      "cache": false
    }
  }
}
