{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "build:packages": {
      "dependsOn": ["^build:packages"],
      "outputs": ["packages/**/dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["build:packages"]
    },
    "test:watch": {
      "cache": false
    },
    "coverage": {}
  }
}
