{
  "name": "@knocklabs/telegraph",
  "version": "0.0.1",
  "description": "The design system at Knock",
  "main": "index.js",
  "repository": "https://github.com/knocklabs/telegraph",
  "author": "@knocklabs",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@4.1.0",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev:packages": "turbo dev --filter=\"./packages/*\" --concurrency=100%",
    "build:packages": "turbo build --filter=\"./packages/*\" --filter=\"!config\"",
    "build:config": "turbo build --filter=\"./packages/*-config\"",
    "dev:examples": "turbo dev:examples --filter=\"./examples/*\"",
    "build:examples": "turbo build:examples --filter=\"./examples/*\"",
    "test": "turbo test --filter=\"./packages/*\"",
    "test:watch": "turbo test:watch --filter=\"./packages/*\"",
    "coverage": "turbo coverage --filter=\"./packages/*\"",
    "lint": "turbo lint",
    "format": "turbo format",
    "format:check": "turbo format:check",
    "dev:storybook": "storybook dev",
    "build:storybook": "storybook build",
    "release": "yarn build:packages && yarn release:publish && yarn changeset tag",
    "release:publish": "yarn workspaces foreach -Rpt --no-private --from '@telegraph/*' npm publish --access public --tolerate-republish",
    "postinstall": "manypkg check",
    "prepare": "husky"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "manypkg": {
    "defaultBranch": "main",
    "ignoredRules": [
      "INTERNAL_MISMATCH"
    ]
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@manypkg/cli": "^0.21.2",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint-plugin-turbo": "^1.12.2",
    "husky": "^9.0.6",
    "prettier": "^3.2.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.17",
    "turbo": "^1.11.3"
  }
}
